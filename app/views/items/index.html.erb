<div class= "paginate">
  <%= paginate @items %>
</div>

<div class="list-wrap">
<% @items.each do |item| %>
  <div class="list-box">
    <%= attachment_image_tag item, :image, :fill, 200, 200, format: 'jpeg' %>
    <div class="list-comment">
      <span class="list-body"><%= item.caption %></span>
      <span class="list-title"><%=link_to item.name, item_path(item.id) %></span>
    </div>
  </div>
<% end %>
</div>

<div class= "paginate">
  <%= paginate @items %>
</div>

</div>